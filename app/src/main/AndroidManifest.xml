    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".content_main"
            android:label="@string/title_activity_content_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".content_main_list" />
        <activity android:name=".Note.NewNoteActivity"/>
        <activity android:name=".Note.NotesListActivity"/>
        <!--<activity android:name=".NoteslistActivity"></activity>-->
    </application>

</manifest>